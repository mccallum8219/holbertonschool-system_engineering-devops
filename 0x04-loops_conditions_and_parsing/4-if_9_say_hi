#!/usr/bin/env bash
#using while and 9th line adds "Best School" "Hi"
#!/usr/bin/env bash
#Bash script that displays "Best School" 10 times
x=1
while [ $x -le 10 ]
do if [ $x -eq 9 ]; then
   echo "Hi"
   else echo#!/usr/bin/env bash
	#Bash script that displays "Best School" 10 times
	x=1
	while [ $x -le 10 ]
	do echo "Best School"
	   x=$(( $x + 1 ))
	   done
